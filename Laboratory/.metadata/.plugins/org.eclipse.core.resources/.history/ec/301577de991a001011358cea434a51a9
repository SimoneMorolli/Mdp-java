package lab3_my_solution;

public class Main {
	public static void main(String[] args) {
		Punto point1 = new Punto(1, 3, 8);
		Punto point2 = new Punto(4, 4, 7);
		
		System.out.println("Queste solo le cordinate del primo punto: " + point1.getCordinate());
		System.out.println("Queste solo le cordinate del secondo punto: " + point2.getCordinate());
		
		Segmento segment1 = new Segmento (point1, point2);
	}
}
